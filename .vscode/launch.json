{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "gdb",
			"request": "launch",
			"name": "Launch Tests",
			"target": "./serve-d-test-library",
			"cwd": "${workspaceRoot}",
			"showDevDebugOutput": true,
			"printCalls": true
		},
		{
			"type": "gdb",
			"request": "attach",
			"name": "Attach to code-d instance",
			"executable": "./serve-d",
			"target": ":2345",
			"remote": true,
			"cwd": "${workspaceRoot}"
		}
	]
}